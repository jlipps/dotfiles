{
  "java.completion.enabled": true,
  "java.configuration.updateBuildConfiguration": "always",
  "java.import.gradle.enabled": true,
  "java.autobuild.enabled": false,
  "java.project.referencedLibraries": "~/.gradle/caches/**",
  "java.maven.downloadSources": true,
  "java.configuration.compliance": "11",
  "tsserver.npm": "~/.nvm/versions/node/v20.17.0/bin",  
  "tsserver.tsdk": "~/.nvm/versions/node/v20.17.0/lib/node_modules/typescript/lib",
  "tsserver.useLocalTsdk": true,
  "python.jediEnabled": false,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--ignore=E501,E731"],
  "python.pythonPath": "/opt/homebrew/bin/python3",
  "Lua.runtime.nonstandardSymbol": ["+=", "-=", "*=", "/="],
  "Lua.workspace.library": ["$PLAYDATE_SDK_PATH/CoreLibs", "./source"],
  "Lua.diagnostics.globals": ["playdate", "json"],
  "Lua.runtime.special": {"import": "require"},
  "Lua.diagnostics.disable": [
    "undefined-field"
  ],
  "java.imports.gradle.wrapper.checksums": [
    {
      "sha256": "1ca04e74ee966719e1155341da6fca086d0d21686db29e3efdebd03817e639e0",
      "allowed": false
    }
  ],
  "svelte.enable-ts-plugin": false
}
